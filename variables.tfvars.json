{
  "name": "ec2-alb",
  "key_name": "root",
  "aws_region": "ap-northeast-2",
  "aws_profile": "default",
  "aws_instance": {
    "ami": "ami-003bb1772f36a39a3",
    "instance_type": "t3.micro",
    "count": 1
  },
  "aws_acm_certificate": {
    "domain": "outqource.com"
  },
  "aws_route53_zone": {
    "name": "outqource.com"
  },
  "aws_route53_record": {
    "name": "test.outqource.com",
    "type": "A",
    "alias": {
      "evaluate_target_health": false
    }
  },
  "aws_vpc": {
    "cidr_block": "10.0.0.0/16",
    "enable_dns_hostnames": true,
    "enable_dns_support": true,
    "instance_tenancy": "default",
    "tags": {
      "Name": "ec2-alb-vpc"
    }
  },
  "aws_subnet": {
    "count": 1,
    "availability_zone": ["ap-northeast-2a", "ap-northeast-2b", "ap-northeast-2c", "ap-northeast-2d"],
    "private_subnet": {
      "Name": "ec2-alb-private-subnet-"
    },
    "public_subnet": {
      "Name": "ec2-alb-public-subnet-"
    }
  },
  "aws_internet_gateway": {
    "name": "ec2-alb-igw",
    "tags": {
      "Name": "ec2-alb-igw"
    }
  },
  "aws_eip": {
    "count": 1,
    "vpc": true,
    "tags": {
      "Name": "ec2-alb-eip-"
    }
  },
  "aws_security_group": {
    "public": {
      "name": "ec2-alb-public-sg",
      "description": "Controls access to the ALB ec2-alb",
      "ingress": [3000, 443, 8000, 80, 3001]
    },
    "private": {
      "name": "ec2-alb-private-sg",
      "description": "Controls access to the ECS ec2-alb"
    }
  },
  "aws_alb": {
    "name": "ec2-alb-alb"
  },
  "aws_alb_target_group": {
    "name": "ec2-alb-target-group",
    "port": 80,
    "protocol": "HTTP",
    "target_type": "ip"
  },
  "aws_alb_target_group_attachment": {
    "ports": [3000, 443, 8000, 80, 3001]
  },
  "aws_alb_listener": {
    "redirect_http": [
      [80, 443],
      [3001, 3000]
    ],
    "origin_http": [3000],
    "origin_https": [443, 8000]
  }
}
